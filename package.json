{
  "name": "sls-static-api",
  "version": "1.0.0",
  "description": "Pre-calculated (fast) API for Brokalys",
  "author": "Matiss Janis Aboltins <matiss@brokalys.com>",
  "license": "MIT",
  "scripts": {
    "start": "sls offline start",
